#include<iostream>
using namespace std;

int main(){
int n;
long sum=0;
//taking the dimension and cointent of array from user

cout<<"Enter the number of items"<<endl;
cin>>n;
int *arr=new int(n);

for(int i=0;i<=n;i++){
    cin>>arr[i];
}

//checking odd or even

for(int i=0;i<n;i++){
    
sum+=arr[i];
    
}
cout<<sum;
return 0;
}
