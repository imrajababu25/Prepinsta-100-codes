#include <iostream>
using namespace std;
void unique(int arr[],int n){
   int ans=0;
   for(int i=0;i<n;i++){
       ans=ans^arr[i];
   }
   cout<<"unique element is "<<ans;
}
int main() {
    int n=7;
	int arr[7]={1,4,2,1,2,3,3};
    int i;
    for(i=0;i<n;i++){
        cout<<arr[i]<<"  ";
    }
    cout<<endl;
    unique(arr,n);
    return 0;
}
